{{#each stations}}
<div class="box box-link-hover-shadow">
  <h2 class="title">
    {{title}}
  </h2>
    {{> weather-summary station=this stationSummary=this.stationSummary weatherInfo=this.weatherInfo}}
  <a href="/station/{{_id}}" class="button">
    {{>icons/open}}
  </a>
  <a href="/dashboard/deletestation/{{_id}}" class="button">
    {{>icons/delete}}
  </a>
</div>
{{/each}}